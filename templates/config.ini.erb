[MGM DEFAULT]
PortNumber=1186
DataDir=<%=data_dir%>/mgm_data

[NDBD DEFAULT]
NoOfReplicas=2
DataDir=<%=data_dir%>/ndb_data

ODirect=1
# change for only has 32 cores
MaxNoOfExecutionThreads=28
RealTimeScheduler=1

#ThreadConfig=ldm={count=8,cpubind=1,2,3,4,5,6,7,8},tc={count=4,cpubind=13,14,15,16},recv={count=4,cpubind=20,21,22,23},send={count=3,cpubind=24,25,26},rep={cpubind=27},main={cpubind=28},io={cpubind=28}
